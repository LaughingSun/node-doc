{
  "name": "node-doc",
  "version": "0.1.1",
  "description": "Node doc is a documenting tool. You can extract documentation from your source code and convert it into markdown, for easy use in for example GitHub.",
  "main": "index.js",
  "scripts": {
    "test": "gulp lint test",
    "coverage": "istanbul cover _mocha test/**.test.js",
    "doc": "nodedoc lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thomasdezeeuw/node-doc"
  },
  "preferGlobal": true,
  "bin": {
    "nodedoc": "./bin/cli.js"
  },
  "keywords": [
    "node",
    "doc",
    "comments",
    "markdown"
  ],
  "author": "Thomas de Zeeuw <thomasdezeeuw@gmail.com> (https://thomasdezeeuw.nl/)",
  "license": "MIT",
  "dependencies": {
    "mkdirp": "^0.3.5",
    "rimraf": "^2.2.6",
    "commander": "^2.1.0"
  },
  "devDependencies": {
    "gulp": "^3.6.0",
    "gulp-mocha": "^0.4.1",
    "gulp-jshint": "^1.5.0",
    "gulp-jscs": "^0.3.2",
    "should": "^3.1.3",
    "mock-fs": "^2.2.0"
  }
}

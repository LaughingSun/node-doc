{
  "name": "node-doc",
  "version": "0.1.4",
  "description": "Node doc is a documenting tool. You can extract documentation from your source code and convert it into markdown, for easy use in for example GitHub.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha 'test/**/*.test.js'",
    "coverage": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/.bin/_mocha 'test/**/*.test.js'",
    "doc": "nodedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thomasdezeeuw/node-doc"
  },
  "preferGlobal": true,
  "bin": {
    "nodedoc": "./bin/cli.js"
  },
  "keywords": [
    "node",
    "doc",
    "comments",
    "markdown"
  ],
  "author": "Thomas de Zeeuw <thomasdezeeuw@gmail.com> (https://thomasdezeeuw.nl/)",
  "license": "MIT",
  "dependencies": {
    "async": "^0.9.0",
    "commander": "^2.1.0",
    "mkdirp": "^0.5.0",
    "rimraf": "^2.2.6"
  },
  "devDependencies": {
    "coveralls": "^2.10.0",
    "gulp": "^3.7.0",
    "gulp-jscs": "^0.5.0",
    "gulp-jshint": "^1.6.0",
    "gulp-mocha": "^0.4.1",
    "istanbul": "^0.2.8",
    "mocha": "^1.20.0",
    "mock-fs": "^2.3.0",
    "should": "^4.0.0"
  }
}
